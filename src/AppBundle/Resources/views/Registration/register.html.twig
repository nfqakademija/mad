<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}{{ "Food planner" }}{% endblock %}</title>
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico')}}">
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/js/materialize.min.js"></script>
    {% block stylesheets2 %}
        <link rel="stylesheet" href="{{ asset('css/style.css') }}">
    {% endblock %}
    {% block scripts %}
        <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
        <script src="{{ asset('bootstrapTheme/js/hover.zoom.js') }}"></script>
        <script src="{{ asset('bootstrapTheme/js/hover.zoom.conf.js') }}"></script>
    {% endblock %}
    {% block canonical_meta %}{% endblock %}
</head>
<body>
{% block header %}
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">Logo</a>
            </div>
        </div>
    </nav>
{% endblock %}
{% block fos_user_content %}
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-8">
                <img src="https://static.pexels.com/photos/5317/food-salad-restaurant-person.jpg" class="img-responsive">
            </div>
            <div class="col-md-4">
                <div class="center-block">
                    <h1 class="register">Registracija</h1>
                    {% include "@FOSUser/Registration/register_content.html.twig" %}
                </div>
            </div>
        </div>
    </div>
{% endblock fos_user_content %}
