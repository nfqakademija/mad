<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}{{ "Food planner" }}{% endblock %}</title>
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico')}}">
    {% block stylesheets2 %}
    <link rel="stylesheet" href="{{ asset('bootstrapTheme/css/bootstrap.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bootstrapTheme/css/main.css') }}">

    {% endblock %}
    {% block scripts %}
    <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
    <script src="{{ asset('bootstrapTheme/js/hover.zoom.js') }}"></script>
    <script src="{{ asset('bootstrapTheme/js/hover.zoom.conf.js') }}"></script>
    {% endblock %}
    {% block canonical_meta %}{% endblock %}
</head>
<body>

    {% block container %}
        {% block header %}
            {% include "::inc/header.html.twig" %}
        {% endblock %}
        {% block body %}{% endblock %}
    {% endblock %}

{% block footer %}
{% include "::inc/footer.html.twig" %}
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('bootstrapTheme/js/bootstrap.min.js') }}"></script>
{% endblock %}
</body>
</html>